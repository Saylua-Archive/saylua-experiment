{
  "extends": "airbnb",
  "env": {
    "browser": true,
  },
  "rules": {
    "object-curly-newline": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "react/destructuring-assignment": 0,
    "no-unused-vars": "warn",
    "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "never",
        "exports": "never",
        "functions": "never"
    }],
  },
}
